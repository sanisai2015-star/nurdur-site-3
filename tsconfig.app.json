{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@sections/*": [
        "./src/components/sections/*"
      ],
      "@layout/*": [
        "./src/components/layout/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@content/*": [
        "./src/content/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}